{
  "name": "nfq-quest-web",
  "description": "NFQ Quest Webapp",
  "author": "cuong.nguyen@nfq.asia",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "vite optimize --force",
    "build": "pnpm prebuild && vue-tsc --noEmit && vite build",
    "build:dev": "pnpm build --mode=dev",
    "build:sta": "pnpm build --mode=staging",
    "preview": "vite preview",
    "vis:bundle": "npx vite-bundle-visualizer",
    "vis:tailwind": "tailwind-config-viewer -p 3033 -c tailwind.config.cjs",
    "vis:system": "npx envinfo --system --npmPackages vite,@vitejs/plugin-vue --binaries --browsers",
    "preinstall": "npx only-allow pnpm",
    "prepare": "concurrently \"husky install\" \"chmod ug+x .husky/*\" \"pnpm:prebuild\"",
    "check:script": "eslint \"**/*.{js,ts,cjs,vue}\" \"src/locales/*.json\"",
    "check:style": "stylelint \"src/**/*.{vue,css,scss}\"",
    "check:format": "prettier --check \"**/*.{js,ts,cjs,vue,css,scss}\" --ignore-path .eslintignore",
    "check:code": "concurrently \"pnpm:check:script\" \"pnpm:check:style\" \"pnpm:check:format\"",
    "check:branch": "concurrently \"chmod 755 .husky/utils/*\" \"sh .husky/utils/check-branch-naming.sh\"",
    "fix:script": "pnpm check:script --fix",
    "fix:style": "pnpm check:style --fix",
    "fix:format": "pnpm check:format --write",
    "fix:code": "concurrently \"pnpm:fix:script\" \"pnpm:fix:style\" \"pnpm:fix:format\""
  },
  "lint-staged": {
    "src/locales/*.json": "eslint",
    "*.{js,ts,cjs,vue}": "eslint",
    "*.{css,scss,vue}": "stylelint",
    "*.{js,ts,cjs,vue,css,scss}": "prettier --check"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 version"
    ]
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "dependencies": {
    "@vueuse/core": "^9.10.0",
    "@vueuse/head": "^1.0.22",
    "@vueuse/router": "^9.10.0",
    "axios": "^1.1.3",
    "axios-cache-interceptor": "^0.10.7",
    "lodash": "^4.17.21",
    "pinia": "^2.0.24",
    "pinia-plugin-persistedstate": "^3.0.2",
    "query-string": "^7.1.1",
    "vue": "^3.2.41",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@intlify/unplugin-vue-i18n": "^0.7.1",
    "@julr/vite-plugin-validate-env": "^0.2.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/lodash": "^4.14.189",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-vue": "^9.7.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "postcss-html": "^1.5.0",
    "prettier": "2.4.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "sass": "^1.55.0",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-prettier": "^2.0.0",
    "tailwind-config-viewer": "^1.7.2",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-vue-components": "^0.22.9",
    "unplugin-vue-router": "^0.2.3",
    "vite": "^3.2.0",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-importus": "^1.0.4",
    "vite-plugin-remove-console": "^1.3.0",
    "vite-plugin-rewrite-all": "^1.0.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-layouts": "0.6.0",
    "vue-tsc": "^1.0.9"
  }
}
